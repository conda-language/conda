
void fizzbuzz(n:int){
  var i:int = 0
  for (i = 0; i <= n; i++){
    if ((i % 3==0) || (i % 5 == 0)){
      if (i % 3 == 0){
        print("Fizz", end="");
      }
      if (i % 5==0){
        print("Buzz", end="");
      } 
    print("");
  }
  else{
    print(i);
    }
  }
}


fizzbuzz(16)
// prints fizzbuzz up to 16 in this case